    <!-- Header -->
    <header>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <img class="img-responsive" src="img/profile.png" alt="">
                    <div class="intro-text">
                        <span class="name">This weeks Donut Baron Is:</span><br />
                        <span class="name"><%= donutBaron.real_name %></span>
                    </div>
                </div>
            </div>
        </div>
    </header>

    <!-- List Section -->
    <section id="list">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2>Upcoming Barons</h2>
                    <hr class="star-light"> <!-- Should make a donut logo or something here -->
                </div>
            </div>
            <div class="row">
                <div class="col-lg-7 col-centered">
                    <table class="table table-striped table-hover">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Date</th>
                            </tr>
                        </thead>
                        <tbody> <!-- Dynamically this section -->
                          <% upcomingUsers.forEach(function(user) { %>
                            <tr>
                              <td>
                            <%= user.real_name %>
                            </td>
                            <td>
                              <%= user.date %>
                            </td>
                            </tr>
                          <% }); %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </section>

    <!-- Rating Section -->
    <section class="success" id="rate">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2>Rate</h2>
                    <hr class="star-light"> <!-- Should make a donut logo or something here -->
                </div>
            </div>
            <div class="row">
                Add donut rating and comment section here
            </div>
        </div>
    </section>

